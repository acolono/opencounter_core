{
    "name": "rosenstrauch/opencounter_api_core",
    "description": "framework agnostic counter business logic",
    "minimum-stability": "dev",
    "version": "0.2.0",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "monolog/monolog": "^1.17",
        "ramsey/uuid": "^3.5"
    },
    "require-dev": {
        "behat/behat": "^3",
        "phpspec/phpspec": "^3",
        "phpdocumentor/phpdocumentor": "^2.9",
        "emuse/behat-html-formatter": "0.1.*",
        "phpdocumentor/reflection-docblock": "^2.0.4",

        "psr/http-message": "^1.0"
    },
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "OpenCounter\\": "src/"
        }
    }
}
